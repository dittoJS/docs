  "scripts": {
    "generate": "NODE_ENV='production' && next build && next export -o docs",
    "dev": "next -p 4001",
    "server": "node ./server/index.js",
    "start": "serve ./",
    "build": "next build",
    "deploy": "yarn generate && touch docs/.nojekyll &&  git add docs/ && git commit -m \"update docs\" && git subtree push --prefix docs origin gh-pages"

  },
