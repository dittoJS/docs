    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "highlight.js": "^9.12.0",
    "marked": "^0.5.1",
    "mds-to-json": "^0.0.4",
    "next": "^7.0.1",
    "node-sass": "^4.9.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "sander": "^0.6.0"
  }
}
